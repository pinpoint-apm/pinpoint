<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="bean1" class="com.navercorp.pinpoint.plugin.lambda.Bean1"/>
    
    <bean id="bean2" class="com.navercorp.pinpoint.plugin.lambda.Bean2">
        <constructor-arg ref="model"/>
    </bean>
    
    <bean id="model" class="com.navercorp.pinpoint.plugin.lambda.Model" scope="prototype">
        <constructor-arg value="4"/>
    </bean>
    
</beans>