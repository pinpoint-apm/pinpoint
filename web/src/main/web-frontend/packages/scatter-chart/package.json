{
  "name": "@pinpoint-fe/scatter-chart",
  "version": "0.2.0-alpha",
  "description": "Open-source scatter-chart library written in pure JS based on canvas api, Specifically designed for visualizing request and response patterns over time",
  "repository": "https://github.com/pinpoint-apm/pinpoint/blob/frontend-v3/web/src/main/web-frontend/packages/scatter-chart/README.md",
  "author": "NAVER",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "browser": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "lint": "eslint",
    "test": "jest",
    "storybook": "start-storybook -p 6009",
    "build-storybook": "build-storybook",
    "build": "tsup --entry.index src/index.ts --entry.scatter-chart src/style/index.css --format esm,cjs",
    "clean": "rm -rf dist",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "devBuild": "tsup src/index.ts --format esm,cjs --dts"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@emotion/babel-plugin": "^11.9.2",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@pinpoint-fe/tsconfig": "*",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/html": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^28.1.4",
    "@types/lodash.merge": "^4.6.7",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.5.4",
    "css-loader": "^6.7.3",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.2",
    "storybook-css-modules": "^1.0.8",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.5",
    "tsup": "^6.2.1",
    "typescript": "^4.5.5"
  }
