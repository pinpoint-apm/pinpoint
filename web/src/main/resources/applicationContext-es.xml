<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="elasticTemplate" class="com.navercorp.pinpoint.web.dao.elasticsearch.ElasticTemplate" init-method="init" destroy-method="destroy">
        <property name="ipAddress" value="${es.host}"/>
        <property name="cluster" value="${es.cluster}"/>
    </bean>
</beans>