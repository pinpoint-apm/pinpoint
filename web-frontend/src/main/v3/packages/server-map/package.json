{
  "name": "@pinpoint-fe/server-map",
  "version": "0.0.11",
  "description": "Open-source network-map library, specifically for application topology, written in React.js",
  "repository": "https://github.com/pinpoint-apm/pinpoint/blob/frontend-v3/web-frontend/src/main/v3/packages/server-map/README.md",
  "author": "NAVER",
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "yarn format && eslint ./src/**/*.ts --config .eslintrc.cjs",
    "test": "jest",
    "storybook": "start-storybook -p 6008",
    "build-storybook": "build-storybook",
    "build": "tsup --config tsup.config.production.ts",
    "clean": "yarn clean:dist && yarn clean:node_modules && yarn clean:turbo",
    "clean:node_modules": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "clean:turbo": "rm -rf .turbo",
    "dev": "tsup --config tsup.config.dev.ts --watch",
    "devBuild": "tsup --config tsup.config.dev.ts"
  },
  "dependencies": {
    "cytoscape": "^3.24.0",
    "cytoscape-dagre": "^2.5.0",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/cytoscape": "^3.19.9",
    "@types/cytoscape-dagre": "^2.3.0",
    "@types/jest": "^28.1.4",
    "@types/react": "^18.0.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.5.4",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.2",
    "prettier": "^3.3.3",
    "ts-jest": "^28.0.5",
    "tsup": "^6.2.1",
    "typescript": "^4.5.5"
  }
}
