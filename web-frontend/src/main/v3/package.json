{
  "name": "pinpoint-fe-v3",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "yarn workspace @pinpoint-fe/web build",
    "dev": "yarn workspace @pinpoint-fe/web dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean --force && rm -rf node_modules",
    "clean:dist": "turbo run clean:dist --force",
    "storybook": "turbo run storybook --force",
    "build:storybook": "turbo run build-storybook",
    "deploy:storybook": "turbo run chromatic"
  },
  "devDependencies": {
    "prettier": "^3.4.2"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=22.13.1"
  },
  "packageManager": "yarn@1.22.22",
  "dependencies": {}
}
